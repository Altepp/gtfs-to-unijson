{
  "name": "gtfs-to-geojson",
  "version": "3.0.1",
  "private": false,
  "description": "Generate geoJSON of transit route data from a GTFS file",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "geojson"
  ],
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-geojson/issues"
  },
  "repository": "git://github.com/blinktaginc/gtfs-to-geojson",
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [
    "Kegan Maher"
  ],
  "type": "module",
  "main": "index.js",
  "bin": {
    "gtfs-to-geojson": "bin/gtfs-to-geojson.js"
  },
  "scripts": {
    "lint": "xo"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "space": true
  },
  "dependencies": {
    "@turf/bbox": "^6.3.0",
    "@turf/bbox-polygon": "^6.3.0",
    "@turf/buffer": "^6.3.0",
    "@turf/convex": "^6.3.0",
    "@turf/helpers": "^6.3.0",
    "@turf/meta": "^6.3.0",
    "@turf/simplify": "^6.3.0",
    "archiver": "^5.3.0",
    "chalk": "^4.1.1",
    "cli-table": "^0.3.6",
    "gtfs": "^3.0.1",
    "lodash-es": "^4.17.21",
    "polygon-clipping": "^0.15.3",
    "pretty-error": "^3.0.3",
    "sanitize-filename": "^1.6.3",
    "timer-machine": "^1.1.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "xo": "0.40.1"
  },
  "engines": {
    "node": ">= 12.14.0"
  }
}
